          data = dff_q %>%
            filter(V_JudgeSelf==0,  V_up==0)),
  "CatchCovid" = lmer(CatchCovid ~ CovidSkepticism * V_Racenamef * EXPGRP_TEXT + (1|ID),
          data = dff_q %>%
            filter(V_JudgeSelf==0,  V_up==0)),
  "TransmitCovid" = lmer(TransmitCovid ~ CovidSkepticism * V_Racenamef * EXPGRP_TEXT + (1|ID),
          data = dff_q %>%
            filter(V_JudgeSelf==0,  V_up==0)))
      
