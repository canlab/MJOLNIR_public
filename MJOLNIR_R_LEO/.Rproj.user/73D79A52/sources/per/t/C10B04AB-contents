reg <- lm(vignette_result ~  V_Location + V_Age + V_Racenamef * EXPGRP_TEXT * V_Product * V_Presentation + V_StoreType + V_Framing, 
          data = dff %>% filter(V_Number==1, V_Question=="JudgeOther_1", V_Update == T))
