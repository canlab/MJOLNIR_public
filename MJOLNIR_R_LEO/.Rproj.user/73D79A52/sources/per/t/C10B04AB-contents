reg <- lm(vignette_result ~ V_Update + V_Racenamef + V_Location + V_Age + V_Product + V_StoreType + V_Framing + EXPGRP_TEXT, 
          data = dff %>% filter(V_Number==1, V_Question=="JudgeOther_1"))
